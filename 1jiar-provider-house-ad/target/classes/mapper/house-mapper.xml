<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.HouseMapper">
    <select id="queryHouseAge" resultType="com.jk.model.HouseAge">
        SELECT
            *
        FROM
            house_age
    </select>
    <insert id="addOwner">
        INSERT INTO t_owner(ownerXiaoqu,ownerSiteDong,ownerSiteDanyuan,ownerSiteMenpai,ownerUserId,ownerSell)
        VALUES
            (#{ownerXiaoqu}, #{ownerSiteDong}, #{ownerSiteDanyuan}, #{ownerSiteMenpai}, #{ownerUserId}, #{ownerSell});
    </insert>
    <insert id="addOwner2" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO house_man(name,phoneNum)
        VALUES
            (#{name}, #{phoneNum});
    </insert>
</mapper>